{
  "name": "sveltekit-dotnet",
  "version": "0.1.0",
  "description": "sveltekit-dotnet sample project",
  "scripts": {
    "dev": "vite dev --port 8019",
    "start": "vite dev",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:pretty": "prettier --check --plugin-search-dir=. . ",
    "test": "echo \"Error: no test specified\" && exit 0",
    "clean": "rimraf ./.svelte-kit && rimraf ./build && rimraf ./node_modules"
  },
  "keywords": [
    "forge",
    "svelte",
    "sveltekit",
    "c#"
  ],
  "type": "module",
  "engines": {
    "node": ">=16.4.0",
    "npm": ">=7.9.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.7.1",
    "@types/cookie": "^0.5.1",
    "esbuild": "^0.16.10",
    "rimraf": "^3.0.2",
    "svelte": "^3.55.0",
    "svelte-adapter-aspcore": "^0.4.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.2"
  }
}
